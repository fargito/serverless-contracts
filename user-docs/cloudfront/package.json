    "dependency-cruiser": "^11.14.0",
    "eslint": "^8.20.0",
    "serverless": "^3.21.0",
    "serverless-lift": "^1.19.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vitest": "^0.23.1"
