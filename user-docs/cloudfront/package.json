{
  "name": "@swarmion/documentation-cloudfront",
  "private": true,
  "version": "0.30.0",
  "license": "MIT",
  "scripts": {
    "format-check": "prettier --check . --ignore-path ../../.prettierignore",
    "format-fix": "prettier --write . --ignore-path ../../.prettierignore",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "nx run format-check && nx run test-linter && nx run test-type && nx run test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc"
  },
  "dependencies": {
    "@swarmion/serverless-configuration": "^0.30.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.30.1",
    "@swarmion/serverless-helpers": "^0.30.0",
    "@types/node": "^18.16.1",
    "dependency-cruiser": "^15.4.0",
    "eslint": "^8.54.0",
    "serverless": "^3.38.0",
    "serverless-lift": "^1.28.1",
    "ts-node": "^10.9.1",
    "typescript": "5.3.2",
    "vitest": "^1.0.0-beta.5"
  }
}
