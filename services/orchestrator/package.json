{
  "name": "@swarmion/orchestrator",
  "private": true,
  "version": "0.26.0",
  "license": "MIT",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy-production": "serverless deploy --stage production",
    "deploy-staging": "serverless deploy --stage staging",
    "destroy": "serverless remove",
    "destroy-production": "serverless remove --stage production",
    "destroy-staging": "serverless remove --stage staging",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-integration": "vitest run --config vitest.integration.config.ts --passWithNoTests",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc",
    "test-unit": "vitest run --coverage --passWithNoTests",
    "test-integration": "vitest run --config vitest.integration.config.ts --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.226.0",
    "@swarmion/orchestrator-contracts": "^0.26.0",
    "@swarmion/serverless-cdk-plugin": "^0.26.0",
    "@swarmion/serverless-configuration": "^0.26.0",
    "@swarmion/serverless-contracts": "^0.26.0",
    "@swarmion/serverless-helpers": "^0.26.0",
    "@swarmion/serverless-plugin": "^0.26.0",
    "ajv": "^8.12.0",
    "aws-cdk-lib": "^2.54.0",
    "aws-sdk": "^2.1272.0",
    "dynamodb-toolbox": "^0.7.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.27.0",
    "@swarmion/eslint-plugin": "^0.26.0",
    "@swarmion/integration-tests": "^0.26.0",
    "@types/node": "^18.11.13",
    "@vitest/coverage-c8": "0.30.0",
    "constructs": "^10.1.187",
    "dependency-cruiser": "^12.1.0",
    "esbuild": "^0.16.0",
    "esbuild-visualizer": "^0.4.0",
    "eslint": "^8.29.0",
    "serverless": "^3.27.0",
    "serverless-analyze-bundle-plugin": "^2.0.0",
    "serverless-custom-iam-roles-per-function": "^1.0.1",
    "serverless-esbuild": "^1.33.2",
    "serverless-iam-roles-per-function": "^3.2.0",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "9.6.0",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "0.30.0"
  }
}
