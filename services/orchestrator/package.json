{
  "name": "@swarmion/orchestrator",
  "private": true,
  "version": "0.31.3",
  "license": "MIT",
  "scripts": {
    "deploy": "echo 'skipping...'",
    "deploy-production": "echo 'skipping...'",
    "deploy-staging": "echo 'skipping...'",
    "destroy": "serverless remove",
    "destroy-production": "serverless remove --stage production",
    "destroy-staging": "serverless remove --stage staging",
    "format-check": "prettier --check . --ignore-path ../../.prettierignore",
    "format-fix": "prettier --write . --ignore-path ../../.prettierignore",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "nx run format-check && nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --config -- .",
    "test-integration": "vitest run --config vitest.integration.config.ts --passWithNoTests",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/client-eventbridge": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@aws-sdk/util-dynamodb": "^3.540.0",
    "@swarmion/orchestrator-contracts": "workspace:^0.31.3",
    "@swarmion/serverless-cdk-plugin": "workspace:^0.31.3",
    "@swarmion/serverless-configuration": "workspace:^0.31.3",
    "@swarmion/serverless-contracts": "workspace:^0.31.3",
    "@swarmion/serverless-helpers": "workspace:^0.31.3",
    "@swarmion/serverless-plugin": "workspace:^0.31.3",
    "ajv": "^8.12.0",
    "aws-cdk-lib": "^2.114.1",
    "dynamodb-toolbox": "^0.9.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.38.0",
    "@swarmion/eslint-plugin": "^0.31.3",
    "@swarmion/integration-tests": "^0.31.3",
    "@types/node": "^20.12.4",
    "@vitest/coverage-v8": "1.5.0",
    "constructs": "^10.3.0",
    "dependency-cruiser": "^16.0.0",
    "esbuild": "0.20.2",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "^8.55.0",
    "node-fetch": "^3.3.2",
    "serverless": "^3.38.0",
    "serverless-analyze-bundle-plugin": "^2.0.0",
    "serverless-better-credentials": "^2.0.0",
    "serverless-custom-iam-roles-per-function": "^1.0.1",
    "serverless-esbuild": "^1.50.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "ts-node": "^10.9.2",
    "ts-toolbelt": "9.6.0",
    "typescript": "5.4.5",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "1.5.0"
  }
}
