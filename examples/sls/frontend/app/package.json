{
  "name": "@sls/frontend-app",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "build": "vite build",
    "format-check": "prettier --check . --ignore-path ../../.prettierignore",
    "format-fix": "prettier --write . --ignore-path ../../.prettierignore",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,jsx,ts,tsx",
    "serve": "vite preview",
    "start": "vite --port 3000",
    "stylelint-base-config": "stylelint",
    "stylelint-fix": "pnpm stylelint-base-config --fix",
    "test": "nx run format-check && nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --config -- src",
    "test-linter": "pnpm linter-base-config src && pnpm test-stylelint",
    "test-stylelint": "pnpm stylelint-base-config '**/*.{ts,tsx}'",
    "test-type": "tsc",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/lab": "5.0.0-alpha.153",
    "@mui/material": "^5.14.18",
    "@mui/system": "^5.14.18",
    "@sls/frontend-shared": "1.0.0",
    "@sls/users-contracts": "1.0.0",
    "@swarmion/serverless-contracts": "^0.30.0",
    "@tanstack/react-query": "^5.8.7",
    "axios": "^1.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^6.5.5",
    "react-router-dom": "^6.20.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/dotenv-flow": "^3.3.3",
    "@types/node": "^18.18.13",
    "@types/react": "^17.0.71",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.0-beta.5",
    "dependency-cruiser": "^15.4.0",
    "dotenv-flow": "^4.0.0",
    "eslint": "^8.54.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-risxss": "^2.1.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "postcss": "^8.4.31",
    "postcss-styled-syntax": "^0.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.0.0",
    "rollup": "^4.5.2",
    "rollup-plugin-visualizer": "^5.9.3",
    "stylelint": "^15.11.0",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "^10.8.1",
    "typescript": "~5.0.4",
    "vite": "^5.0.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.0.0-beta.5"
  }
}
