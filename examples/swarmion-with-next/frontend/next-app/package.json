{
  "name": "@swarmion-with-next/frontend-next-app",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "bootstrap": "cdk bootstrap",
    "bootstrap-production": "cdk bootstrap --context stage=production",
    "bootstrap-staging": "cdk bootstrap --context stage=staging",
    "build": "next build",
    "deploy": "cdk deploy",
    "deploy-production": "cdk deploy --context stage=production",
    "deploy-staging": "cdk deploy --context stage=staging",
    "destroy": "cdk destroy",
    "destroy-production": "cdk destroy --context stage=production",
    "destroy-staging": "cdk destroy --context stage=staging",
    "dev": "next dev",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts,jsx,tsx .",
    "start": "next start",
    "stylelint-base-config": "stylelint",
    "stylelint-fix": "pnpm stylelint-base-config --fix",
    "test": "nx run test-linter && nx run test-type && nx run test-unit && nx run test-circular",
    "test-circular": "pnpm depcruise --validate .dependency-cruiser.js src",
    "test-linter": "pnpm linter-base-config && pnpm stylelint-base-config **/*.{ts,tsx}",
    "test-type": "tsc --noEmit --emitDeclarationOnly false",
    "test-unit": "jest --maxWorkers=2 --collectCoverage --logHeapUsage --passWithNoTests"
  },
  "dependencies": {
    "@emotion/styled": "^11.10.5",
    "@mui/lab": "5.0.0-alpha.118",
    "@mui/material": "^5.11.7",
    "@mui/system": "^5.11.7",
    "@tanstack/react-query": "^4.24.4",
    "axios": "^1.3.2",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.7"
  },
  "devDependencies": {
    "@aws-cdk/aws-amplify-alpha": "2.63.2-alpha.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.19",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.5",
    "aws-cdk": "^2.63.2",
    "aws-cdk-lib": "^2.63.2",
    "constructs": "^10.1.242",
    "dependency-cruiser": "^12.7.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-risxss": "^2.1.0",
    "eslint-plugin-testing-library": "^5.10.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "postcss": "^8.4.21",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
